<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="org.ntierbiz.core.user.model.BasicAddressPO" table="BASIC_ADDRESS" >
		<id name="id" type="long" >
		 <column name="ID" />
			<generator class="identity" />
		</id>	
		<many-to-one name="basicProfile" class="org.ntierbiz.core.user.model.BasicProfilePO" >
			<column name="PROFILE_ID" not-null="true"/>
		</many-to-one>
		<property name="typeCode" column="TYPE_CODE" type="int"/>
		<property name="addressName" column="ADDRESS_NAME" type="java.lang.String"/>
		<property name="addressNr" column="ADDRESS_NR" type="java.lang.String"/>
		<property name="city" column="CITY" type="java.lang.String"/>
		<property name="country" column="COUNTRY" type="java.lang.String"/>
		<property name="countryCode" column="COUNTRY_CODE" type="java.lang.String"/>
		<property name="county" column="COUNTY" type="java.lang.String"/>
		<property name="postBox" column="POST_BOX" type="java.lang.String"/>				
		<property name="postalCode" column="POSTAL_CODE" type="java.lang.String"/>
		<property name="region" column="REGION" type="java.lang.String"/>
		<property name="province" column="PROVINCE" type="java.lang.String"/>
		<property name="state" column="STATE" type="java.lang.String"/>
		<property name="street" column="STREET" type="java.lang.String"/>
		<property name="street2" column="STREET2" type="java.lang.String"/>
		<property name="streetTranscription" column="STREET_TRANSCRIPTION" type="java.lang.String"/>
		<property name="email" column="EMAIL" type="java.lang.String"/>
		<property name="fax" column="FAX" type="java.lang.String"/>
		<property name="homePage" column="HOME_PAGE" type="java.lang.String"/>
		<property name="mobile" column="MOBILE" type="java.lang.String"/>
		<property name="phoneHome" column="PHONE_HOME" type="java.lang.String"/>
		<property name="phoneBusiness" column="PHONE_BUSINESS" type="java.lang.String"/>
		<property name="phoneBusinessDirect" column="PHONE_BUSINESS_DIRECT" type="java.lang.String"/>
		<property name="companyName" column="COMPANY_NAME" type="java.lang.String"/>
		<property name="companyNameTranscription" column="COMPANY_NAME_TRANSCRIPTION" type="java.lang.String"/>
		<property name="firstName" column="FIRST_NAME" type="java.lang.String"/>
		<property name="firstNameTranscription" column="FIRST_NAME_TRANSCRIPTION" type="java.lang.String"/>
		<property name="secondName" column="SECOND_NAME" type="java.lang.String"/>
		<property name="honorific" column="HONORIFIC" type="java.lang.String"/>
		<property name="jobTitle" column="JOB_TITILE" type="java.lang.String"/>
		<property name="lastName" column="LAST_NAME" type="java.lang.String"/>
		<property name="lastNameTranscription" column="LAST_NAME_TRANSCRIPTION" type="java.lang.String"/>
		<property name="secondLastName" column="SECOND_LAST_NAME" type="java.lang.String"/>
		<property name="suffix" column="SUFFIX" type="java.lang.String"/>
		<property name="title" column="TITLE" type="java.lang.String"/>
		<property name="DomainId" column="DOMAIN_ID" type="long"/>
		<property name="createdBy" column="CREATED_BY" type="java.lang.String"/>
		<property name="modifiedBy" column="MODIFIED_BY" type="java.lang.String"/>
		<property name="lastModified" column="LAST_MODIFIED" type="java.sql.Date"/>
		<filter name="filterByDefaultAddress" condition="typeCode=1" />
	</class>
</hibernate-mapping>
